<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Loggers>
		<Root level="DEBUG">
			<AppenderRef ref="CONSOLE"/>
		</Root>
	</Loggers>
	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT">
			<PatternLayout pattern="[%5p|%d{HH:mm:ss}] %m (%F:%L)%n"/>
		</Console>
	</Appenders>
</Configuration>